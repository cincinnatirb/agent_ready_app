<div id="<%= dom_id structure %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Nickname:</strong>
    <%= structure.nickname %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Building type:</strong>
    <%= structure.building_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Length:</strong>
    <%= structure.length %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Width:</strong>
    <%= structure.width %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Parcel:</strong>
    <%= structure.parcel_id %>
  </div>
</div>
